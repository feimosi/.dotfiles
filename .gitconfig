[user]
    name = Marek Grzybek
    email = feimosi@onet.pl
[credential]
    helper = wincred
[core]
    autocrlf = true
[push]
    default = simple
[alias]
    a = add
    l = log --graph --oneline --decorate
    touched = diff-tree -r --name-only --no-commit-id
    s = status -s -b --
    c = checkout
    cm = commit -m
    cp = cherry-pick
    r = rebase
    m = merge
